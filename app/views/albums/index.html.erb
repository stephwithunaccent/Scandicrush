<div class="container-fluid">
  <div class="col-xs-3">
    <div class="navbar-wagon-right hidden-xs hidden-sm">
      <div class=title>
       <h4>
         <% @albums.each do |album| %>
           <%= link_to album_path(album) do %>
            <li><%= album.name %></li>
           <% end %>
         <% end %>
       </h4>
     </div>
    </div>
 </div>

 <div class="col-xs-9">
  <div class="gallery landing">
    <div class="gallery-img">
      <%= link_to album_path(@album) do %>
  <%= image_tag "C.jpg", :class=>"mySlides", style: "width:100%";  %>
      <% end %>
      <div>
  <%= image_tag "E.jpg", :class=>"mySlides", style: "width:100%";  %>
      </div>
    </div>
  </div>
 </div>
</div>
